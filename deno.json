{
  "tasks": {
    "install": "deno install --allow-scripts",
    "prisma": "npx prisma generate",
    "start": "deno run --watch --allow-read --allow-env --allow-net --allow-sys --allow-ffi ./src/main.ts",
    "docker": "docker compose up"
  },
  "imports": {
    "@nestjs/apollo": "npm:@nestjs/apollo@^12.2.0",
    "@nestjs/cli": "npm:@nestjs/cli@^10.4.5",
    "@nestjs/common": "npm:@nestjs/common@^10.4.5",
    "@nestjs/core": "npm:@nestjs/core@^10.4.5",
    "@nestjs/graphql": "npm:@nestjs/graphql@^12.2.0",
    "@nestjs/microservices": "npm:@nestjs/microservices@^10.4.6",
    "@nestjs/platform-express": "npm:@nestjs/platform-express@^10.4.5",
    "@prisma/client": "npm:@prisma/client@^5.21.1",
    "apollo-server-express": "npm:apollo-server-express@^3.13.0",
    "dotenv": "npm:dotenv@^16.4.5",
    "graphql": "npm:graphql@^16.9.0",
    "prisma": "npm:prisma@5.21.1",
    "reflect-metadata": "npm:reflect-metadata@^0.2.2",
    "rxjs": "npm:rxjs@^7.8.1",
    "class-validator": "npm:class-validator@^0.14.1",
    "class-transformer": "npm:class-transformer@^0.5.1",
    "ioredis": "npm:ioredis@^5.4.1",
    "@elastic/elasticsearch": "npm:@elastic/elasticsearch@^8.15.1",
    "jsonwebtoken": "npm:jsonwebtoken@^9.0.2",
    "kafkajs": "npm:kafkajs@^2.2.4"
  },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
